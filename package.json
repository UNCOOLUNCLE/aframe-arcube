{
  "name": "aframe-trl-starter-kit",
  "description": "ARCube",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && ./node_modules/.bin/webpack-cli --entry ./src/index.js --output dist/aframe-arcube.min.js",
    "dev": "./node_modules/.bin/budo src/index.js:main.js --dir public --port 7000 --live",
    "start": "npm run dev"
  },
  "keywords": [
    "aframe",
    "aframe-js",
    "ar",
    "arcube",
    "webar"
  ],
  "dependencies": {
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.10"
  },
  "devDependencies": {
    "budo": "^11.2.0"
  }
}
